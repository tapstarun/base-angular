{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",

      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.mp4"
        ],
        "urls": [
          "*.jwpcdn.com/**",
          "*.jwplayer.com/**",
          "*.jwplatform.com/**",
          "*.jwpltx.com/**",
          "*.jwpsrv.com/**",
          "*.mux.com/**",
          "*.videos-fms.jwpsrv.com/**",
          "*.videos-cloudflare.jwpsrv.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "quiz",
      "urls": [],
      "cacheConfig": {
        "maxAge": "6h",
        "maxSize": 5,
        "strategy": "performance",
        "timeout": "10s"
      }
    }
  ]
}
